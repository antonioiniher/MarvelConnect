<div class="container">
    {{#each users}}
    <div class="card" style="width: 18rem;">
        <img src="{{imageUrl}}" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">{{username}}</h5>
            <a href="/usuario/{{id}}" class="btn btn-primary">Ir al perfil</a>
        </div>
    </div>
    {{/each}}
</div>